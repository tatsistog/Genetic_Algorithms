#!/home/togkousa/PycharmProjects/Training_1/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'sugartex==0.1.16','console_scripts','sugartex'
__requires__ = 'sugartex==0.1.16'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('sugartex==0.1.16', 'console_scripts', 'sugartex')()
    )
